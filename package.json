{
  "name": "applications-service",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "node ./bin/www",
    "startDev": "DEBUG=applications-service:* nodemon ./bin/www",
    "test": "eslint . && jest .",
    "format": "prettier --single-quote --write {apps,models,services}/**/* README.md"
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn",
      "node/no-unpublished-require": "off",
      "node/shebang": "off"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "connect-session-sequelize": "^5.2.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.3",
    "nunjucks": "^3.1.3",
    "object-hash": "^1.3.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.12",
    "sequelize": "^4.38.0",
    "sqlite3": "^4.0.1"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-plugin-node": "^6.0.1",
    "faker": "^4.1.0",
    "jest": "^23.3.0",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.7"
  }
}
