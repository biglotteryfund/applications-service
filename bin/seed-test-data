let json = {
    formId: 'TEST',
    shortCode: 'DEV-',
    applicationData: application
};

for (let i = 0; i < 100; i++) {

    let appJson = Object.assign({}, json);
    appJson.applicationData[0].version = i;
    appJson.shortCode += i;

    request({
        url: "http://localhost:3001/api/application/add",
        method: "POST",
        json: true,
        body: appJson
    }, function (error, response, body) {
        if (error) {
            console.log(error);
            console.log('error');
        } else {
            console.log(body);
            console.log('success!');
        }
    });

}


